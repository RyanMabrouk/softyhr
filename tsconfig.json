{
  "compilerOptions": {
    "downlevelIteration": true,
    "declaration": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(dashboard)/Files/_ui/filesTable/tableSideBar/TableSideBarFolderBox.tsx",
    "src/components/ui/Pagination.tsx",
    "src/app/(dashboard)/people/(employees)/orgchart/_ui/components/OrgChart/OrgChartBody.tsx",
    "src/app/(dashboard)/people/(employees)/orgchart/_ui/components/OrgChart/prevversion.jsx",
    "src/app/(dashboard)/Files/context/CheckBoxesContext.js",
    "src/app/(dashboard)/Files/_ui/components/pdfviewer/PdfViewer.jsx"
  ],
  "exclude": ["node_modules"]
}
